{
    "edge_id": "{{ edge_id }}",
    "command": "docker",
    "params": "start",
    "config":[
        {
            "image": "{{ image_repo }}",
            "container_name": "{{ container_name }}",
            "detach": "True",
            "binding_port":{ {%for port in port_mapping %}
                "{{ port.con_port }}/{{ port.port_protocol }}":"{{ port.phy_port }}"{{ "," if not loop.last else "" }} {% endfor %}
            }
        }
    ],
    "conf_path": "{{ conf_path }}"
}